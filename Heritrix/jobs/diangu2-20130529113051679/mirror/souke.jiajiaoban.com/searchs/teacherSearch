<script type="text/javascript">
function submitJiabaobanSearch()
{
	var kemu = $('#kemu').val();
	var nianji = $('#nianji').val();
	var xuequ = $('#xuequ').val();
	var teachername = $('#teachername').val();
	var jiabaobanSearchUrl = "/searchs/search/tuiguang:/kemu:" + kemu + "/nianji:" + nianji + "/xuequ:" + xuequ + "/teachername:" + teachername;
	window.open(jiabaobanSearchUrl);
}
</script> 
