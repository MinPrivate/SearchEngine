<script type="text/javascript" language="javascript">

    //搜索分数线(下拉单)
    function spepselect(stuarea,subject,year,spename,schname){
        if(stuarea!=''){
            stuarea = '/a'+stuarea;
        }
        if(subject!=''){
            subject = '/s'+subject;
        }
        if(year!=''){
            year = '/y'+year;
        }
        if(spename!=''){
            spename = '/n'+spename;
        }
        if(schname!=''){
            schname = '/o'+schname;
        }

        var page = $("#pagenum").val();
        if(page!=''){
            page = '/p'+page;
        }

        var url = "http://college.gaokao.com/spepoint"+stuarea+subject+year+spename+schname+page+"/";
        window.location.href=url;
    }

    //跳转到url
    function gotourl(){
        if($("#spename").val() == "请输入专业名称"){
            var spename = "";
        }else{
            var spename = $("#spename").val();
        }
        if($("#schname").val() == "请输入高校名称"){
            var schname = "";
        }else{
            var schname = $("#schname").val();
        }
        spepselect($("#stuarea").val(),$("#subject").val(),$("#year").val(),spename,schname);
    }

    $(document).ready(function(){

        //取得select框内数据
        $.getJSON("http://college.gaokao.com/json/area?id=",function(data){ 
            $("#stuarea").html(data.json);
        });
        $.getJSON("http://college.gaokao.com/json/subject?id=",function(data){ 
            $("#subject").html(data.json);
        });

        //搜索分数线
        $("#spepselect").click(function(){
            _gaq.push(['_trackEvent', 'College', 'search', 'spepli']);
            gotourl();
        });

        //跳转到X页
        $("#pagego").click(function(){
            gotourl();
        });

        //显示全部考生所在地
        $("#showAllB").click(function(){
            $("#showAllArea").show();
            $("#showNoArea").hide();
        });

        //回车事件 回车触发
        $('body').bind('keydown', function (e) {
            var key = e.which;
            if(key==13){
                gotourl();
            }
        });

    });

</script>
